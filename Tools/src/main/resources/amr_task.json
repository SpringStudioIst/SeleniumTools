{
  "title": "辅助注册",
  "des": "2020-12-30",
  "url": "https://amr.sz.gov.cn/aicmerout/jsp/gcloud/giapout/industry/aicmer/processpage/step_prewin.jsp",
  "config": {
    "maximize": true
  },
  "wordList": [
    "angcyo",
    "!!angcyo521"
  ],
  "actionList": [
    {
      "title": "点击[我要申办]",
      "start": "1000,0",
      "check": {
        "event": [
          {
            "tags": "button",
            "textList": [
              "我要申办"
            ]
          }
        ],
        "handle": [
          {
            "filter": {
              "index": "0"
            },
            "actionList": [
              "click"
            ]
          }
        ]
      }
    },
    {
      "title": "点击[帐号密码]",
      "start": "1000,0",
      "check": {
        "event": [
          {
            "tags": "a",
            "textList": [
              "^账号密码$"
            ]
          }
        ],
        "handle": [
          {
            "filter": {
              "index": "0"
            },
            "actionList": [
              "click"
            ]
          }
        ]
      }
    },
    {
      "title": "输入账号",
      "start": "1000,0",
      "check": {
        "event": [
          {
            "cssList": [
              "input[placeholder=请输入账号]"
            ]
          }
        ],
        "handle": [
          {
            "filter": {
              "index": "0"
            },
            "actionList": [
              "input:$0"
            ]
          }
        ]
      }
    },
    {
      "title": "输入密码",
      "start": "1000,0",
      "check": {
        "event": [
          {
            "cssList": [
              "input[placeholder=请输入密码]"
            ]
          }
        ],
        "handle": [
          {
            "filter": {
              "index": "0"
            },
            "actionList": [
              "input:$1"
            ]
          }
        ]
      }
    },
    {
      "title": "获取[验证码]图片",
      "start": "300,0",
      "check": {
        "event": [
          {
            "cssList": [
              "input[placeholder=请输入验证码]"
            ],
            "filter": {
              "relativeBy": "img:toRightOf:0"
            }
          }
        ],
        "handle": [
          {
            "filter": {
              "index": "0"
            },
            "actionList": [
              "getAttr[src]:imageCode"
            ]
          }
        ]
      }
    },
    {
      "title": "解析验证码",
      "start": "300,0",
      "check": {
        "handle": [
          {
            "actionList": [
              "code:$[imageCode]:imageCode"
            ]
          }
        ]
      }
    },
    {
      "title": "输入验证码",
      "start": "300,0",
      "check": {
        "event": [
          {
            "cssList": [
              "input[placeholder=请输入验证码]"
            ]
          }
        ],
        "handle": [
          {
            "filter": {
              "index": "0"
            },
            "actionList": [
              "input:$[imageCode]"
            ]
          }
        ]
      }
    },
    {
      "title": "点击[登录]",
      "start": "1000,0",
      "check": {
        "event": [
          {
            "tags": "button",
            "textList": [
              "^登录$"
            ]
          }
        ],
        "handle": [
          {
            "filter": {
              "index": "0"
            },
            "actionList": [
              "click"
            ]
          }
        ]
      }
    }
  ],
  "backActionList": [
  ]
}